pipeline {
    agent any
    stages {
        stage('run') {
            steps {
                echo 'Clarusway_Way to Reinvent Yourself'
                sh 'python --version'
                sh 'python pipeline.py'
            }
        }
        stage('Sumyee') {
            steps {
                echo 'Welcome to symeyyee stage'
                sh 'python sumyee.py'
            }
        }
        stage('Abbi') {
            steps {
                echo 'welcome to abbi stage'
                sh 'python abbi.py'
            }
        }
        stage('Hiba') {
            steps {
                echo 'welcome to Hiba stage'
                sh 'python abbi.py'
            }
        }
    }
}